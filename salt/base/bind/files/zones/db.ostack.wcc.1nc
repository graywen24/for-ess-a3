;
; BIND data file for the 1nc domain
;
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	   IN NS dns-a1.cde.1nc.
@	   IN NS dns-a2.cde.1nc.

dns-a1               IN     A    10.3.50.118
dns-a2               IN     A    10.3.50.119

compute-b1        IN   A   10.3.34.14
compute-b2        IN   A   10.3.34.13
